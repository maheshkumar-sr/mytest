{
  "name": "nike-poc",
  "version": "1.0.0",
  "description": "Nike POC for PO Search",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "webpack:config": "webpack --config webpack.config.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "build": "npm-run-all --parallel webpack:*",
    "clean:all": "rimraf ./build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "npm run build && npm run clean:all",
    "flow": "flow",
    "flow:all": "flow --show-all-errors",
    "flow:stop": "flow stop",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@date-io/date-fns": "1.3.13",
    "@hot-loader/react-dom": "^16.12.0",
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "css-loader": "^3.4.2",
    "date-fns": "^2.10.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "jest": "^25.2.3",
    "jest-ratchet": "^2.0.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "flow-bin": "^0.121.0",
    "jest-enzyme": "^7.1.2",
    "react-test-renderer": "^16.13.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/tools/jest/setup.js"
    ],
    "moduleNameMapper": {
      ".*\\.(css|scss|sass)$": "<rootDir>/tools/jest/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/jest/assetMock.js"
    }
  }
}
